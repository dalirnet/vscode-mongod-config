{
    "name": "mongod-config",
    "displayName": "Mongod config",
    "description": "Syntax highlight and formatter for Mongod config files",
    "publisher": "dalirnet",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages",
        "Formatters"
    ],
    "icon": "icon.png",
    "keywords": [
        "mongo",
        "mongod",
        "conf",
        "cfg"
    ],
    "activationEvents": [
        "onLanguage:mongod-config"
    ],
    "main": "./scripts/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "mongod-config",
                "aliases": [
                    "Mongod config",
                    "mongod-config"
                ],
                "extensions": [
                    "conf",
                    ".conf",
                    "cfg",
                    ".cfg"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mongod-config",
                "scopeName": "source.mongod-config",
                "path": "./syntaxes/mongod-config.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dalirnet/vscode-mongod-config.git"
    }
}
